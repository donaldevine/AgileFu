<h1>Listing product_backlogs</h1>

<table class="table">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Iteration</th>
    <th>Reason</th>
    <th>Priority</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_backlogs.each do |product_backlog| %>
  <tr>
    <td><%= product_backlog.title %></td>
    <td><%= product_backlog.description %></td>
    <td><%= product_backlog.iteration %></td>
    <td><%= product_backlog.reason %></td>
    <td><%= product_backlog.priority %></td>
    <td><%= link_to 'Show', project_product_backlog_path(product_backlog.project, product_backlog)  %></td>
    <td><%= link_to 'Edit', edit_project_product_backlog_path(product_backlog.project, product_backlog) %></td>
    <td><%= link_to 'Destroy', project_product_backlog_path(product_backlog.project, product_backlog), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

