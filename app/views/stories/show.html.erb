<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @story.title %>
</p>

<p>
  <b>Description:</b>
  <%= @story.description %>
</p>

<p>
  <b>Project:</b>
  <%= @story.project_id %>
</p>

<p>
  <b>Priority:</b>
  <%= @story.priority %>
</p>

<p>
  <b>Best case estimate:</b>
  <%= @story.best_case_estimate %>
</p>

<p>
  <b>Worst case estimate:</b>
  <%= @story.worst_case_estimate %>
</p>


<%= button_to "Edit", edit_story_path(@story), :method => "get", :class => "btn btn-default" %>
<%= button_to "Back", stories_path, :method => "get", :class => "btn btn-default" %>
