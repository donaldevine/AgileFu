<h1>My Projects</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Target Start</th>
    <th>Target End</th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name, edit_project_path(project) %></td>
    <td><%= project.description %></td>
    <td><%= project.target_start_date %></td>
    <td><%= project.target_completion_date %></td>
  </tr>
<% end %>
</table>

<br />

<%= button_to "New Project", new_project_path, :method => "get", :class => "btn btn-default" %>

